@model List<ResultCategoryDto>

<section class="section" id="offers">
    <div class="container">
        <div class="row">
            <div class="col-lg-4 offset-lg-4 text-center">
                <div class="section-heading">
                    <h6>Klassy` de Bu Hafta</h6>
                    <h2></h2>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="row" id="tabs">
                    <div class="col-lg-12">
                        <div class="heading-tabs">
                            <div class="row">
                                <div class="col-lg-12">
                                        <ul>
                                        @foreach (var item in Model)
                                        {
                                            <li><a href="/Default/GetProductListByCategory/@item.CategoryId"><img src="~/klassy_cafe/assets/images/tab-icon-01.png" alt="">@item.CategoryName</a></li>
                                        }
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
